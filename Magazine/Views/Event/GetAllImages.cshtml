@model IEnumerable<Magazine.MOdel.ImageFolder.ImageListItem>

@{
    ViewBag.Title = "GetAllImages";
}
<br />
<br />

<div class=" ">
    <h2>
        @ViewBag.List1
    </h2>
    <p>
       @ViewBag.List
    </p>

</div>


<div class="row">

    @foreach (var item in Model)
    {

        <div>

            @if (item.FileContent != null)
            {
                var base64 = Convert.ToBase64String(item.FileContent); var imgsrc = String.Format("data:image/gif;base64,{0}", base64);<img src=@imgsrc ,class="posting" />
            }

            else
            {
                <img src="" class="img-fluid" alt="" />
            }


            <p>
                @Html.DisplayFor(modelItem => item.ImgageInfo)
            </p>



        </div>

    }

</div>




